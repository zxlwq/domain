# Neon数据库的完整连接字符串
DATABASE_URL=postgres://user:password@host:5432/dbname

# 管理后台密码
PASSWORD=123456

# Cloudflare API密钥（用于导入Cloudflare域名）
CF_KEY=cloudflare_api_token

# Telegram通知（可选）
TG_BOT_TOKEN=telegram_bot_token
TG_USER_ID=telegram_user_id

# Server酱/微信推送（可选）
WECHAT_SENDKEY=wechat_sendkey

# Qmsg酱/QQ推送（可选）
QMSG_KEY=qmsg_key
QMSG_QQ=qq_number

# WebDAV同步（可选，用于备份/恢复域名列表）
WEBDAV_URL=https://your.webdav.server/
WEBDAV_USER=webdav_user
WEBDAV_PASS=webdav_password